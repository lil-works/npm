                            <b class="caret"></b>
                        </a>
                        <ul class="dropdown-menu">
                            {{ render(controller("ManagerBundle:Default:menu")) }}
                        </ul>
                    </li>
                    <li>
                        {% include "FOSUserBundle::navbar.html.twig" %}
